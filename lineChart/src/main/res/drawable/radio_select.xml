<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@color/purple_500_" />
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:end="2dp"
        android:start="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <solid android:color="@color/purple_00" />
        </shape>
    </item>

    <item
        android:bottom="6dp"
        android:end="6dp"
        android:start="6dp"
        android:top="6dp">
        <shape android:shape="oval">
            <solid android:color="@color/purple_500_" />
        </shape>
    </item>

</layer-list>